<definition>
	<command prose="I open the URL ${url}" name="open">
		<execute function="Open" locator1="${url}" />
	</command>

	<command prose="I search for ${input}" name="search">
		<execute function="Type" locator1="//input[@title='Search']" value="${input}" />
		<execute function="Click" locator1="//input[@title='Search']" value="${input}" />
	</command>

	<command prose="I search for ${input}" name="search2">
		<execute function="Type" locator1="//input[@title='Search']" value="${input}" />
		<execute function="Click" locator1="//input[@title='Search']" value="${input}" />
	</command>

	<command prose="I should see ${assert} and ${check} and ${verify}" name="assertSearch">
		<execute function="AssertTextPresent" locator1="//body" value="${assert}" />
	</command>
</definition>