<definition>
	<command name="catSearch">
		<execute macro="LocalFile.PoshiProseFileTest#visitURL">
			<var name="url" value="https://www.yahoo.com"/>
		</execute>
		<execute macro="LocalFile.PoshiProseFileTest#issueSearch">
			<var name="input" value="cats"/>
		</execute>
		<execute macro="LocalFile.PoshiProseFileTest#assertSearchResults">
			<var name="result1" value="stray cat"/>
			<var name="result2" value="domestic cat"/>
			<var name="result3" value="kitten"/>
			<var name="result4" value="lion"/>
		</execute>
	</command>
	<command name="assertLiferayMainPageAvailability">
		<execute macro="LocalFile.PoshiProseFileTest#visitURL">
			<var name="url" value="https://www.liferay.com"/>
		</execute>
		<execute macro="LocalFile.PoshiProseFileTest#wait">
			<var name="element" value="title"/>
		</execute>
		<execute macro="LocalFile.PoshiProseFileTest#closeWindow"/>
	</command>
</definition>