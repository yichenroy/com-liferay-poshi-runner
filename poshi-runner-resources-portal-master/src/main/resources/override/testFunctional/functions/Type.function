<definition default="type" summary="Type to '${locator1}' the value '${value1}'">
	<command name="clickAtSendKeys">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.Type#clickAtSendKeys" />

		<execute argument1="1000" selenium="pause" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="clickAtType">
		<execute function="WaitForSPARefresh" />

		<execute function="Poshi.Type#clickAtType" />

		<execute argument1="1000" selenium="pause" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="sendKeys">
